<% unless @activities.blank? %>
  <%if @latest %>
    $("#feed").html("<%= escape_javascript(render("index")) %>")
  <%else%>
    $("#feed").append("<%= escape_javascript(render("index")) %>")
   
    <% if @activities.next_page %>
      $('.pagination').replaceWith('<%= j will_paginate(@examples) %>');
    <% else %>
      $('.pagination').remove();
    <% end %>
  <%end%>
  
<%else%>
  $("#feed").html("no recent updates");
<%end%>  
