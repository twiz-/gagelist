<div class="row nav-under-wrapper">
  <% if !user_signed_in? %>
    <ul class="nav-under-not-home pull-right">
     <li><%= link_to "Home", root_path  %></li>  
     <li><%= link_to "What it does", does_path  %></li>
     <li><%= link_to "Benefits of using", benefits_path  %></li>
     <li id="right-link"><%= link_to "What it costs", pricing_path  %></li>
    <% end %>
  </ul>
</div>
<div class="row" style="margin-top: 60px;">
  <div class="span4 offset4">
    <div class="span4 signup_well">
      <legend> Sign in</legend>
      <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
        <%= f.email_field :email, :placeholder => "Your email" %>
        <%= f.password_field :password, :placeholder => "Your Password" %><br>
        <% if devise_mapping.rememberable? -%>
         <%= f.check_box :remember_me %> <span class="help-inline"><%= f.label :remember_me %></span><br><br>
        <% end -%>
        <%= f.submit "Sign in", :class => "btn" %>
      <% end %>
    </div>  
  </div>  
</div>
<div class="offset5"><small><%= render "devise/shared/links" %></small></div>

<%= render 'layouts/footer' %>